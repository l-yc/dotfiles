! rofi config below
rofi.theme:                          onedark-customised
rofi.modi:                           window,run,drun,ssh
rofi.dpi:                            144
!rofi.width:                          40
rofi.lines:                          8
rofi.columns:                        2
!rofi.font:                           source code pro 14
!rofi.theme:                          onedark-customised
!rofi.color-normal:                   #09152D, #ffffe9, #09152D, #cbcb5f, #ffffe9
!rofi.color-urgent:                   #09152D, #d6b48d, #22231D, #d6b48d, #ffffe9
!rofi.color-active:                   #09152D, #3b7ef8, #09152D, #3b7ef8, #09152D
!rofi.color-window:                   argb:cc09152D, argb:cccbcb5f, argb:cc8c8c41
!rofi.bw:                             0
!rofi.location:                       0
!!rofi.padding:                        350
!rofi.line-padding:                   10
!rofi.fixed-num-lines:                true
rofi.terminal:                       urxvt256c
rofi.ssh-command:                    {terminal} -e {ssh-client} pi@{host} -p 8022
!rofi.parse-hosts:                    true
rofi.matching:                       fuzzy
!rofi.separator-style:                dash
!rofi.hide-scrollbar:                 true
