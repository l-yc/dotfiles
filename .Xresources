Xft.dpi: 144

!URxvt.font: xft:Monospace:size=10
URxvt.font:xft:DejaVu Sans Mono for Powerline:size=10,xft:WenQuanYi Bitmap Song:pixelsize=18;antialias=false
URxvt.boldFont:xft:DejaVu Sans Mono for Powerline:size=10:style=Bold,xft:WenQuanYi Bitmap Song:pixelsize=18:style=Bold:antialias=false
!URxvt.font: xft:DejaVu\ Sans\ Mono\ for\ Powerline:pixelsize=22,xft:PowerlineSymbols
!URxvt.font: xft:Monospace:size=18
!URxvt.perl-ext-common: ...,tabbed,...
URxvt.perl-ext-common: tabbed
URxvt.scrollBar: false
URxvt.tabbed.tabbar-fg: 15
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 208
URxvt.tabbed.tab-bg: 0
URxvt.internalBorder: 10
